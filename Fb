Minus.MouseButton1Up:Connect(function()
	mousemode = false
	closest = false
	if target and target.Character and target.Character:FindFirstChild("lol3") then
		target.Character:FindFirstChild("lol3"):Destroy()
			end
			if target and target.Character and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
				target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel"):Destroy()
					end
					if target and target.Character and target.Character:FindFirstChild("BillboardGui") then
						target.Character:FindFirstChild("BillboardGui"):Destroy()
							end
							Mouse.TextColor3 = Color3.fromRGB(255, 0, 0)
							Mouse.BorderColor3 = Color3.fromRGB(255, 0, 0)
							TargetClosests.TextColor3 = Color3.fromRGB(255, 0, 0)
							TargetClosests.BorderColor3 = Color3.fromRGB(255, 0, 0)
							wait()
							switchMINUS()
end)
Plus_2.MouseButton1Up:Connect(function()
	steps += 5
	Steps.Text = steps
end)
Minus_2.MouseButton1Up:Connect(function()
	if steps > 5 then
	steps -= 5
	Steps.Text = steps
	end
end)
Plus_3.MouseButton1Up:Connect(function()
	interval += 0.01
	Interval.Text = string.format("%.2f", interval)
end)

Minus_3.MouseButton1Up:Connect(function()
	if interval > 0.01 then
	interval -= 0.01
	Interval.Text = string.format("%.2f", interval)
		end
end)
m.Button2Down:Connect(function()
	if mousemode and m.Hit then
	for _, v in pairs(game.Players:GetPlayers()) do
		if v ~= plr and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and v.Character:FindFirstChild("HumanoidRootPart") and (v.Character:FindFirstChild("HumanoidRootPart").Position - m.Hit.Position).Magnitude < 25 then
			target = v
			Target.Text = target.Name
			if v ~= target then
	if target and target.Character and target.Character:FindFirstChild("lol3") then
		target.Character:FindFirstChild("lol3"):Destroy()
			end
			if target and target.Character and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
				target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel"):Destroy()
					end
					if target and target.Character and target.Character:FindFirstChild("BillboardGui") then
						target.Character:FindFirstChild("BillboardGui"):Destroy()
							end
							end
							end
							end
							end
end)
spawn(function()
	while true do
		for i = 255, 0, -2.5 do game.CoreGui.ScreenGui.Frame.BorderColor3 = Color3.fromRGB(i, 0, 0) wait() end
		wait(1)
		for i = 0, 255, 2.5 do game.CoreGui.ScreenGui.Frame.BorderColor3 = Color3.fromRGB(i, 0, 0) wait() end
		wait(1)
		end
	end)
spawn(function()
	while true do
		update()
		wait(1)
		end
	end)
spawn(function()
	while true do
		if closest then
	local distance = math.huge
	for _, v in pairs(game.Players:GetPlayers()) do
		if v ~= plr and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and v.Character:FindFirstChild("HumanoidRootPart") and plr.Character and plr.Character:FindFirstChild("Humanoid") and plr.Character:FindFirstChild("Humanoid").Health > 0 and plr.Character:FindFirstChild("HumanoidRootPart") and (plr.Character:FindFirstChild("HumanoidRootPart").Position - v.Character:FindFirstChild("HumanoidRootPart").Position).magnitude < distance then
			target = v
			distance = (plr.Character:FindFirstChild("HumanoidRootPart").Position - v.Character:FindFirstChild("HumanoidRootPart").Position).magnitude
				Target.Text = target.Name
				if v ~= target then
	if target and target.Character and target.Character:FindFirstChild("lol3") then
		target.Character:FindFirstChild("lol3"):Destroy()
			end
			if target and target.Character and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
				target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel"):Destroy()
					end
					if target and target.Character and target.Character:FindFirstChild("BillboardGui") then
						target.Character:FindFirstChild("BillboardGui"):Destroy()
							end
							end
							end
							end
							esp(target)
							end
							if target ~= nil and target.Character then
	if target.Character:FindFirstChild("lol3") ~= nil then
		if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and target.Character:FindFirstChild("Humanoid") and target.Character:FindFirstChild("Humanoid").Health > 0 then
			local move = nil
			for _, n in pairs(target.Character:GetChildren()) do
				if n.ClassName == "Tool"then
				move = n.Name
				end
				end
				if move then
	local bool = false
	for _, v in pairs(blockablemoves) do
		if move == v then
	if target.Character:FindFirstChild("lol3") and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
		target.Character:FindFirstChild("lol3").OutlineColor = Color3.fromRGB(0, 255, 0)
			target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel").Text = v
				target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel").TextColor3 = Color3.fromRGB(0, 255, 0)
					bool = true
					end
					end
					end
					if bool == false and target.Character:FindFirstChild("lol3") and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
						target.Character:FindFirstChild("lol3").OutlineColor = Color3.fromRGB(255, 0, 0)
							target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel").Text = move
								target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel").TextColor3 = Color3.fromRGB(255, 0, 0)
									end
									else
									if target.Character:FindFirstChild("lol3") and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
										target.Character:FindFirstChild("lol3").OutlineColor = Color3.fromRGB(0, 0, 0)
											target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel").Text = ""
												target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel").TextColor3 = Color3.fromRGB(0, 0, 0)
													end
													end
													end
													else
													esp(target)
													end
													end
													wait()
													end
end)

RunService.RenderStepped:Connect(function()
	local character = target.Character
	if checkPlayerState(character) then
	local pos = character.HumanoidRootPart.Position
	local groundY = getGroundY(pos)
	if groundY and chams then
	local shadowPos = Vector3.new(pos.X, groundY + 0.025, pos.Z)
	shadow.CFrame = CFrame.new(shadowPos) * CFrame.Angles(0, 0, math.rad(90))
	shadow.Transparency = 0.25
	else
	shadow.Transparency = 1
	end
	else
	shadow.Transparency = 1
	end
end)


spawn(function()
	game:GetService("RunService").Heartbeat:Connect(function()
		checkRagdollState()
		end)
end)
UIS.InputEnded:Connect(function(i)
	if i.KeyCode == Enum.KeyCode.Minus then
	game.CoreGui.ScreenGui.Frame.Visible = false
	end
	if i.KeyCode == Enum.KeyCode.Equals then
	game.CoreGui.ScreenGui.Frame.Visible = true
	end
end)
local UIS = game:GetService('UserInputService')
local frame = Frame
local dragToggle = nil
local dragSpeed = 0.25
local dragStart = nil
local startPos = nil

local function updateInput(input)
	local delta = input.Position - dragStart
	local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
		startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
end

frame.InputBegan:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
	dragToggle = true
	dragStart = input.Position
	startPos = frame.Position
	input.Changed:Connect(function()
		if input.UserInputState == Enum.UserInputState.End then
	dragToggle = false
	end
	end)
	end
	end)

	UIS.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	if dragToggle then
	updateInput(input)
	end
	end
end)
