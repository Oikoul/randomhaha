local UIS = game:GetService("UserInputService")
	local target = nil
	local plrtbl = {}
	local plr = game.Players.LocalPlayer
	local currentIndex = 1
	local blockablemoves = {"Light Kick", "Guzan", "Jewels Of Light", "Fire Fist", "Fire Gun", "Pillar", "Beam", "Major eruption", "Meteor Volcano", "Pressure Cannon", "Sumo Thrust Barrage", "Fullbright", "Fujiazami", "Phoenix Inferno", "Bluebird", "Cupid's Arrows", "Blossom Wind", "Island Shake", "Launch", "Planetary Devastation", "Dragon Breath", "Tatsumaki", "Jet Gatling", "Ice Hail", "Absolute Zero", "Hound Blaze", "Magma Rain", "Scorching Fist", "Supernova", "Piercing Shine", "Photon Storm", "Heavenly Descent", "Chloro Frenzy", "Venom Road", "Poison Geyser", "Grilled Dough", "Lotus Dough Combo", "Anemone", "Lightning Palm", "Claw Barrage", "Wreckage", "Hurricane Shock", "Neo Red Hawk", "Roc Gatling", "Dawn Rocket", "Mole Pistol Combo", "Inferno Breath", "Enthral Grasp", "Zeus Meadow", "Crimson Pillar", "Anti Quake", "Black Hole Path", "Primal Rage", "Arctic Breath", "Hallowed Cuts", "Elastic Lasso", "Blazing Storm", "Finger Pistol Barrage", "Infinity Drive"}
		local m = plr:GetMouse()
		local names = true
		local chams = true
		local predict = false
		local closest = false
		local mousemode = false
		local trajectoryFolder = nil
		local steps = 5
		local interval = 0.20
		local dist = math.huge
		local RunService = game:GetService("RunService")

			for _, v in pairs(game.Players:GetPlayers()) do
				if v ~= plr and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("Humanoid").Health > 0 and v.Character:FindFirstChild("HumanoidRootPart") and plr.Character and plr.Character:FindFirstChild("Humanoid") and plr.Character:FindFirstChild("Humanoid").Health > 0 and plr.Character:FindFirstChild("HumanoidRootPart") and (plr.Character:FindFirstChild("HumanoidRootPart").Position - v.Character:FindFirstChild("HumanoidRootPart").Position).magnitude < dist then
					target = v
					dist = (plr.Character:FindFirstChild("HumanoidRootPart").Position - v.Character:FindFirstChild("HumanoidRootPart").Position).magnitude
				end
			end
			if target then
				Target.Text = target.Name
			end
			local function showPath(velocity)
				if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and predict then
					if not trajectoryFolder then
					trajectoryFolder = Instance.new("Folder")
		trajectoryFolder.Name = "RagdollTrajectories"
		trajectoryFolder.Parent = workspace
		end
		local position = target.Character:FindFirstChild("HumanoidRootPart").Position
			local gravity = workspace.Gravity
			local currentVelocity = velocity

			for i = 1, steps do
				local part = Instance.new("Part")
					part.Size = Vector3.new(0.5, 0.5, 0.5)
					part.Shape = Enum.PartType.Ball
					part.Anchored = true
					part.CanCollide = false
					part.Material = Enum.Material.SmoothPlastic
					part.Color = Color3.fromRGB(255, 0, 0)
					position = position + (currentVelocity * interval)
					currentVelocity = currentVelocity + Vector3.new(0, -gravity * interval, 0)
					part.Position = position
					part.Parent = trajectoryFolder
					end
					end
			end

			local function checkRagdollState()
				if target and target.Character and target.Character:FindFirstChild("Humanoid") and target.Character:FindFirstChild("HumanoidRootPart") and predict then
					local isRagdolled = target.Character:FindFirstChild("Humanoid"):GetAttribute("IsRagdolled")

						if isRagdolled then
	if isRagdolled ~= 0 then
	local velocity = target.Character:FindFirstChild("HumanoidRootPart").AssemblyLinearVelocity
		showPath(velocity)
		else
		if trajectoryFolder then
	trajectoryFolder:Destroy()
	trajectoryFolder = nil
	end
	end
	end
	end
end
local function esp(targ)
	if chams then
	if target and target.Character and target.Character:FindFirstChild("lol3") then
		target.Character:FindFirstChild("lol3"):Destroy()
			end
			local ch = Instance.new("Highlight")
				ch.OutlineColor = Color3.fromRGB(0, 0, 0)
				ch.FillColor = Color3.fromRGB(0, 0, 0)
				ch.OutlineTransparency = 0
				ch.FillTransparency = 1
				ch.Name = "lol3"
				ch.Enabled = true
				ch.Adornee = targ.Character
				ch.Parent = targ.Character
				end
				if names then
	if target and target.Character and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
		target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel"):Destroy()
			end
			if target and target.Character and target.Character:FindFirstChild("BillboardGui") then
				target.Character:FindFirstChild("BillboardGui"):Destroy()
					end
					if target and target.Character and target.Character:FindFirstChild("Humanoid") and target.Character:FindFirstChild("Humanoid").Health > 0 and target.Character:FindFirstChild("Head") then
						local billboardGui = Instance.new("BillboardGui")
							billboardGui.Adornee = target.Character:FindFirstChild("Head")
								billboardGui.Size = UDim2.new(0, 100, 0, 50)
								billboardGui.StudsOffset = Vector3.new(0, 7, 0)
								billboardGui.Parent = target.Character
								local label = Instance.new("TextLabel")
									label.Size = UDim2.new(1, 0, 1, 0)
									label.BackgroundTransparency = 1
									label.Text = ""
									label.TextColor3 = Color3.new(1, 1, 1)
									label.TextScaled = true
									label.Parent = billboardGui
									end
									end
end
local function update()
	table.clear(plrtbl)
	for _, v in pairs(game.Players:GetPlayers()) do
		if v and v ~= plr then
	table.insert(plrtbl, v)
	end
	end
	for _, v in pairs(game.Players:GetPlayers()) do
		if v and v ~= plr and v ~= target and v.Character then
	for _, n in pairs(v.Character:GetChildren()) do
		if v ~= plr and v ~= target and n and n.Name == "lol3" or n.Name == "BillboardGui" then
		n:Destroy()
		end
		end
		end
		end
	end
	local function switchPLUS()
		if target and target.Character then
	if target and target.Character and target.Character:FindFirstChild("lol3") then
		target.Character:FindFirstChild("lol3"):Destroy()
			end
			if target and target.Character and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
				target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel"):Destroy()
					end
					if target and target.Character and target.Character:FindFirstChild("BillboardGui") then
						target.Character:FindFirstChild("BillboardGui"):Destroy()
							end
							end
							print(plrtbl[currentIndex])
							target = plrtbl[currentIndex]
							currentIndex = (currentIndex % #plrtbl) + 1
							Target.Text = target.Name
							if target and target.Character and target.Character:FindFirstChild("Humanoid") and target.Character:FindFirstChild("HumanoidRootPart") and target.Character:FindFirstChild("Humanoid").Health > 0 then
								esp(target)
								end
end
local function switchMINUS()
	if target and target.Character then
	if target and target.Character and target.Character:FindFirstChild("lol3") then
		target.Character:FindFirstChild("lol3"):Destroy()
			end
			if target and target.Character and target.Character:FindFirstChild("BillboardGui") and target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel") then
				target.Character:FindFirstChild("BillboardGui"):FindFirstChild("TextLabel"):Destroy()
					end
					if target and target.Character and target.Character:FindFirstChild("BillboardGui") then
						target.Character:FindFirstChild("BillboardGui"):Destroy()
							end
							end
							print(plrtbl[currentIndex])
							target = plrtbl[currentIndex]
							currentIndex = (currentIndex % #plrtbl) - 1
							Target.Text = target.Name
							if target and target.Character and target.Character:FindFirstChild("Humanoid") and target.Character:FindFirstChild("HumanoidRootPart") and target.Character:FindFirstChild("Humanoid").Health > 0 then
								esp(target)
								end
end
local shadow = Instance.new("Part")
	shadow.Size = Vector3.new(1, 5, 5)
	shadow.Shape = Enum.PartType.Cylinder
	shadow.Anchored = true
	shadow.CanCollide = false
	shadow.Material = Enum.Material.SmoothPlastic
	shadow.Color = Color3.fromRGB(255, 0, 0)
	shadow.Transparency = 0.25
	shadow.Name = "PlayerShadow"
	shadow.Parent = workspace
	local function getGroundY(position)
		local origin = position + Vector3.new(0, 5, 0)
		local direction = Vector3.new(0, -400, 0)
		local raycastParams = RaycastParams.new()
		raycastParams.FilterDescendantsInstances = {shadow, target.Character}
		raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

		local result = workspace:Raycast(origin, direction, raycastParams)
		if result then
	return result.Position.Y
	end
	return nil
end
local function checkPlayerState(character)
	if character and character:FindFirstChild("Humanoid") and character.Humanoid.Health > 0 then
		if character.HumanoidRootPart.Position.Y < workspace.FallenPartsDestroyHeight then
	return false
	end
	return true
	end
	return false
end

Pred.MouseButton1Up:Connect(function()
	if predict then
	predict = false
	wait(.1)
	if trajectoryFolder then
	trajectoryFolder:Destroy()
	trajectoryFolder = nil
	end
	Pred.TextColor3 = Color3.fromRGB(255, 0, 0)
	Pred.BorderColor3 = Color3.fromRGB(255, 0, 0)
	else
	predict = true
	Pred.TextColor3 = Color3.fromRGB(0, 255, 0)
	Pred.BorderColor3 = Color3.fromRGB(0, 255, 0)
	end
end)
Chams.MouseButton1Up:Connect(function()
	if chams then
	chams = false
	if target and target.Character and target.Character:FindFirstChild("lol3") then
		target.Character:FindFirstChild("lol3"):Destroy()
			end
			Chams.TextColor3 = Color3.fromRGB(255, 0, 0)
			Chams.BorderColor3 = Color3.fromRGB(255, 0, 0)
			else
			chams = true
			Chams.TextColor3 = Color3.fromRGB(0, 255, 0)
			Chams.BorderColor3 = Color3.fromRGB(0, 255, 0)
			end
end)
Mouse.MouseButton1Up:Connect(function()
	if mousemode then
	mousemode = false
	Mouse.TextColor3 = Color3.fromRGB(255, 0, 0)
	Mouse.BorderColor3 = Color3.fromRGB(255, 0, 0)
	else
	mousemode = true
	closest = false
	TargetClosests.TextColor3 = Color3.fromRGB(255, 0, 0)
	TargetClosests.BorderColor3 = Color3.fromRGB(255, 0, 0)
	Mouse.TextColor3 = Color3.fromRGB(0, 255, 0)
	Mouse.BorderColor3 = Color3.fromRGB(0, 255, 0)
	end
end)
