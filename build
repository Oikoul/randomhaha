local mous = game.Players.LocalPlayer:GetMouse()
local target = nil
local spam = false
local function start()
	while spam == true do
		local v = target
		local pos = v.Position
		local dist = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - pos).magnitude
		if dist < 145 then
			local epi = .25
			local orgcal = dist / 22
			if dist > 85 and dist <= 125 then
				orgcal = orgcal * 1.5
				epi = .5
			end
			if dist > 125 and dist <= 140 then
				orgcal = orgcal * 1.75
				epi = .7
			end
			if dist > 140 and dist <= 145.5 then
				orgcal = orgcal * 2
				epi = .9
			end
			local Future = pos + (v.Velocity * epi)
			local args = {[1] = Vector3.new(Future.x, pos.y + orgcal, Future.z),[2] = 1,[3] = 76653.4595537}
			game:GetService("ReplicatedStorage").Modules.ProjectileHandler.Event:FireServer(unpack(args))
			epi = .1
		end
		wait(waitt)
	end
end
