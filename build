local function forcestart()
	while attracting == true do
		if line == true and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
			local nuz = 0
			for _, v in pairs(info) do
				if v ~= nil then
					nuz += 1
				end
			end
			nuz -= 1
			local disst = (nuz * times) /-2
			local nuzz = 0
			for _, v in pairs(info) do
				if v ~= nil and v:FindFirstChild("BodyPosition") ~= nil and v:FindFirstChild("BodyGyro") and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
					local fut = e.Character:FindFirstChild("HumanoidRootPart").CFrame + (e.Character:FindFirstChild("HumanoidRootPart").Velocity * .4)
					local xx = fut * CFrame.new(disst + nuzz, 0, 0)
					local zz = fut * CFrame.new(disst + nuzz, 0, 0)
					nuzz +=  times
					v.BodyGyro.CFrame = e.Character:FindFirstChild("Head").CFrame
					v.BodyPosition.Position = Vector3.new(xx.x + LeOrRight, fut.y + heightt, zz.z)
				end
			end
		end
		if spinn == true and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
			local set = 0
			for _, v in pairs(info) do
				if v ~= nil then
					set += 1
				end
			end
			local setup = 360 / set
			local items = 0
			for _, v in pairs(info) do
				if v ~= nil and v:FindFirstChild("BodyPosition") ~= nil and v:FindFirstChild("BodyGyro") and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
					local fut = e.Character:FindFirstChild("HumanoidRootPart").Position + (e.Character:FindFirstChild("HumanoidRootPart").Velocity * .4)
					local ea = spinsp * 50
					local xx = fut.x + ea * math.cos(math.rad(spinspp + setup * items))
					local yy = fut.y
					local zz = fut.z - ea * math.sin(math.rad(spinspp + setup * items))
					spinspp += spinsp
					items += 1
					v.BodyPosition.Position = Vector3.new(xx + LeOrRight, yy + heightt, zz)
					v.BodyGyro.CFrame = e.Character:FindFirstChild("Head").CFrame
				end
			end
		end
		if line == false and spinn == false and attracting == true and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
			for _, v in pairs(info) do
				if v ~= nil and v:FindFirstChild("BodyPosition") ~= nil and v:FindFirstChild("BodyGyro") ~= nil and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > 0 then
					local fut = e.Character:FindFirstChild("HumanoidRootPart").CFrame + (e.Character:FindFirstChild("HumanoidRootPart").Velocity * .4)
					local xx = fut * CFrame.new(LeOrRight, 0, 0)
					local zz = fut * CFrame.new(LeOrRight, 0, 0)
					v.BodyPosition.Position = Vector3.new(xx.x, fut.y + heightt, zz.z)
					v.BodyGyro.CFrame = e.Character:FindFirstChild("Head").CFrame
				end
			end
		end
		game:GetService("RunService").Heartbeat:Wait()
	end
end
local function CreateSG(name,parent,face)
	local SurfaceGui = Instance.new("SurfaceGui",parent)
	SurfaceGui.Parent = parent
	SurfaceGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	SurfaceGui.Face = Enum.NormalId[face]
	SurfaceGui.LightInfluence = 0
	SurfaceGui.ResetOnSpawn = false
	SurfaceGui.Name = name
	SurfaceGui.AlwaysOnTop = true
	local Frame = Instance.new("Frame",SurfaceGui)
	Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Frame.Size = UDim2.new(1,0,1,0)
	Frame.BorderColor3 = Color3.fromRGB(255, 0, 0)
	Frame.BorderSizePixel = 7
	Frame.BackgroundTransparency = 0.9
end

local ended = false
local function chamm12()
	while wait(5) and chamm1 == true do
		if chamm1 == true then
			ended = false
			for i,v in pairs (game:GetService("Players"):GetPlayers()) do
				if v ~= game:GetService("Players").LocalPlayer and 
					v.Character ~= nil and 
					v.Character:FindFirstChild("Head") and 
					_G.chams == true and
					v.Character.Head:FindFirstChild("lol1") == nil and
					v.Name ~= game.Players.LocalPlayer.Name then
					for i,v in pairs(v.Character:GetChildren()) do
						if v:IsA("MeshPart") or v.Name == "Head" or v:IsA("Part") then
							CreateSG("lol1",v,"Back")
							CreateSG("lol1",v,"Front")
							CreateSG("lol1",v,"Left")
							CreateSG("lol1",v,"Right")
							CreateSG("lol1",v,"Top")
							CreateSG("lol1",v,"Bottom")
						end
					end

				end
			end
		end
		if chamm1 == false and ended == false then
			for i, b in pairs(game:GetDescendants()) do
				if b:IsA("SurfaceGui") and b.Name == "lol1" then
					b:Destroy()
				end
			end
			ended = true
		end
	end
end

