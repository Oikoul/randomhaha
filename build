CreatePart.MouseButton1Click:Connect(function()
	if created == false then
		local a = Instance.new("Part")
		a.Parent = workspace
		a.Size = Vector3.new(2, 2, 2)
		a.Transparency = .5
		a.Anchored = true
		a.CanCollide = false
		local value1 = game.Players.LocalPlayer.Character.Head.Position.X
		local value2 = game.Players.LocalPlayer.Character.Head.Position.Y
		local value3 = game.Players.LocalPlayer.Character.Head.Position.Z
		local roundedValue1 = math.floor(value1)
		if value1 - roundedValue1 >= 0.5 then
			roundedValue1 = roundedValue1 + 1
		end

		local roundedValue2 = math.floor(value2)
		if value2 - roundedValue2 >= 0.5 then
			roundedValue2 = roundedValue2 + 1
		end

		local roundedValue3 = math.floor(value3)
		if value3 - roundedValue3 >= 0.5 then
			roundedValue3 = roundedValue3 + 1
		end
		a.Position = Vector3.new(roundedValue1, roundedValue2, roundedValue3)
		a.Name = "selectedPart"
		created = true
		selectedPart = workspace:FindFirstChild("selectedPart")
		arrowX = createArrow("X", Color3.fromRGB(255, 0, 0)) -- Red arrow along X axis
		arrowY = createArrow("Y", Color3.fromRGB(0, 255, 0)) -- Green arrow along Y axis
		arrowZ = createArrow("Z", Color3.fromRGB(0, 0, 255)) -- Blue arrow along Z axis
		updateArrows()----
	end
end)

