local function build()
	local e = game.Players.LocalPlayer.Character.LowerTorso
	local ypos = game.Players.LocalPlayer.Character.LowerTorso.Position.Y - 2
	game:GetService("ReplicatedStorage").Events.PlaceStructure:FireServer(Vector3.new(e.Position.x, e.Position.y - 2, e.Position.z), "Plant Box", 0, false)
	wait(.5)
	while building == true do
		for _, v in pairs(game.Workspace.Deployables:GetChildren()) do
			if v.Name == "Plant Box" and v:FindFirstChild("Compost") ~= nil and (e.Position - v.Compost.Position).magnitude < 5 then
				local f = v.Compost
				for xx = -7, 7, 7 do
					for zz = -7, 7, 7 do
						game:GetService("ReplicatedStorage").Events.PlaceStructure:FireServer(Vector3.new(f.Position.x + xx, ypos, f.Position.z + zz), "Plant Box", 0, false)
					end
				end
			end
		end
		wait()
	end
end

