						target = plr.Character.HumanoidRootPart
						spam = true
						print("Started")
						start()
					end
				end
			end
		else
			spam = false
			target = nil
			print("Stopped")
		end
	end
end)
local selectedPart = workspace
local arrowParts = {} -- Table to store arrow parts
local moveIncrement = 1
local currentArrow = nil
local plr = game:WaitForChild("Players").LocalPlayer
local pressedON = nil
local offset1 = 0
local offset2 = 0
local mouse = plr:GetMouse()



-- Function to create an arrow along a specific axis
local function createArrow(axis, color, offset)
	local arrow = Instance.new("Part")
	arrow.Name = "Arrow_" .. axis
	arrow.Anchored = true
	arrow.Size = Vector3.new(1, 1, 1) -- Set the arrow size to 6 studs in length (twice as big)
	arrow.Color = color
	arrow.Shape = Enum.PartType.Ball
	arrow.Transparency = 0 -- Ensure arrows are fully visible
	arrow.CanCollide = false
	arrow.Parent = selectedPart -- Parent the arrow to the selected part

	-- Add the arrow to the arrowParts table for easy access
	arrowParts[axis] = arrow
	return arrow
end

-- Create arrows along each axis
local arrowX = workspace
local arrowY = workspace
local arrowZ = workspace

-- Function to update arrow positions
local function updateArrows()
	local x = selectedPart.Size.X / 2
	local y = selectedPart.Size.Y / 2
	local z = selectedPart.Size.Z / 2
	arrowX.Position = selectedPart.Position + Vector3.new(x + 2, 0, 0) -- Adjust arrow X position
	arrowY.Position = selectedPart.Position + Vector3.new(0, y + 2, 0) -- Adjust arrow Y position
	arrowZ.Position = selectedPart.Position + Vector3.new(0, 0, z + 2) -- Adjust arrow Z position
end
mouse.Button1Down:Connect(function(k)
		if mouse.Target == arrowX then
			pressedON = "x"
			local value = arrowX.Position.X
			local roundedValue = math.floor(value)
			if value - roundedValue >= 0.5 then
				roundedValue = roundedValue + 1
			end
			offset1 = roundedValue
		elseif mouse.Target == arrowY then
			pressedON = "y"
			local value = arrowX.Position.Y
			local roundedValue = math.floor(value)
			if value - roundedValue >= 0.5 then
				roundedValue = roundedValue + 1
			end
			offset1 = roundedValue
		elseif mouse.Target == arrowZ then
			pressedON = "z"
			local value = arrowX.Position.Z
			local roundedValue = math.floor(value)
			if value - roundedValue >= 0.5 then
				roundedValue = roundedValue + 1
			end
			offset1 = roundedValue
		end
end)

