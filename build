local function autopick1()
	while autopick == true do
		if autopick == true then
			local chr = game.Players.LocalPlayer.Character
			local items = {}
			for _, pick in pairs(game.Workspace:GetChildren()) do
				if pick:FindFirstChild("Pickup") then
					for _, pick1 in pairs(pick:GetChildren()) do
						if pick1:IsA("Part") and (chr.HumanoidRootPart.Position - pick1.Position).magnitude < 80 then
							table.insert(items, pick)
						end
					end
				end
			end
			for _, pick in pairs(game.Workspace.Items:GetChildren()) do
				if pick.ClassName ~= "Model" and (chr.HumanoidRootPart.Position - pick.Position).magnitude < 80 then
					table.insert(items, pick)
				end
			end
			for _, pick in pairs(game.Workspace.Deployables:GetDescendants()) do
				if pick.Name == "Pickup" and pick.Parent.Parent.Parent.Name == "Chest" and (chr.HumanoidRootPart.Position - pick.Parent.Parent.Parent.Base.Position).magnitude < 80 then
					table.insert(items, pick.Parent)
				end
			end

			for _, pick in pairs(items) do
				game:GetService("ReplicatedStorage").Events.Pickup:FireServer(pick, pick.ClassName)
			end
		end
		wait(2)
	end
end
local function autoheal1()
	while autoheal == true do
		local popo = 0
		for _, v in pairs(game.Players.LocalPlayer.PlayerGui.MainGui.RightPanel.Inventory.List:GetChildren()) do
			popo += 1
			if v.Name == "Bloodfruit" then
				local popo = popo - 1
				healarg = popo
			end
		end
		if autoheal == true then
			if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health ~= nil and game.Players.LocalPlayer.Character.Humanoid.Health < 100 and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health ~= 0 then
				repeat
					game:GetService("ReplicatedStorage").Events.UseBagItem:FireServer(tonumber(healarg))
					wait()
				until game.Players.LocalPlayer:FindFirstChild("Character") == nil or game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") == nil or game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("Health") == nil or game.Players.LocalPlayer.Character.Humanoid.Health == 100 or game.Players.LocalPlayer.Character.Humanoid.Health == 0 or autoheal == false
			end
		end
		wait()
	end
end

