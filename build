local partss = {}
local _x = 0
local _y = 0
local _z = 0
local plut = nil

local function rumn()
	partss = {}
	_x = 0
	_y = 0
	_z = 0
	local region = Region3.new(workspace.selectedPart.Position - workspace.selectedPart.Size / 2, workspace.selectedPart.Position + workspace.selectedPart.Size / 2)
	local partsinregion = workspace:FindPartsInRegion3(region, workspace.selectedPart, 100000)
		for _, v in pairs(partsinregion) do
			if v ~= nil and v.ClassName == "Part" and v.Name == "Hitbox" or v.Name == "Part" and v.Parent and v.Parent.Parent and v.Parent.Parent.Name == "Objects" and v.Parent.Parent.Parent and v.Parent.Parent.Parent.Parent and v.Parent.Parent.Parent.Parent.Name == "Plots" then
				table.insert(partss, v.Parent)
			end
		end
		local a = workspace.selectedPart.Position
		_x = a.x
		_y = a.y
		_z = a.z
end

local function movemmd()
	local ea = workspace.selectedPart.Position
	local xx = ea.x - _x
	local yy = ea.y - _y
	local zz = ea.z - _z
	for _, v in pairs(partss) do
		for _, part in pairs(v:GetChildren()) do
			if part ~= nil and part.ClassName == "Part" and part.Name == "Part" or part.Name == "Hitbox" and plut then
				local args = {
						[1] = v,
						[2] = {
							[1] = v.Name,
							[2] = part.CFrame + Vector3.new(xx, yy, zz),
							[3] = plut
						}
					}
					game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Move"):InvokeServer(unpack(args))
					print("moved")
				end
			end
		end
end

local function buildmd()
	local ea = workspace.selectedPart.Position
	local xx = ea.x - _x
	local yy = ea.y - _y
	local zz = ea.z - _z
		for _, v in pairs(partss) do
			for _, part in pairs(v:GetChildren()) do
				if part ~= nil and part.ClassName == "Part" and part.Name == "Part" or part.Name == "Hitbox" and plut then
					local args = {
						[1] = v.Name,
						[2] = part.CFrame + Vector3.new(xx, yy, zz),
						[3] = plut
					}

					game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Place"):InvokeServer(unpack(args))
					print("built")
				end
			end
		end
end

selectt.MouseButton1Click:Connect(function()
	rumn()
end)
Movemm.MouseButton1Click:Connect(function()
	movemmd()
end)
Buildd.MouseButton1Click:Connect(function()
	buildmd()
end)
TEXTT.FocusLost:Connect(function(i)
	for _, v in pairs(game.Players:GetPlayers()) do
		if string.lower(TEXTT.Text) == string.lower(v.Name) then
			local plot = v.Values.Plot.Value
			for _, n in pairs(workspace.Plots:GetChildren()) do
				if n.Name == tostring(plot) then
					plut = n
					TEXTT.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
					print("Plot: "..plut.Name)
				end
			end
		elseif string.lower(TEXTT.Text) == "me" then
			local plot = game:GetService("Players").LocalPlayer.Values.Plot.Value
			for _, n in pairs(workspace.Plots:GetChildren()) do
				if n.Name == tostring(plot) then
					plut = n
					TEXTT.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
					print("Plot: "..plut.Name)
				end
			end
		else
			TEXTT.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
		end
	end
end)
